{
  "name": "ker0x/cakephp-push",
  "type": "cakephp-plugin",
  "description": "CakePHP plugin to send push notification through services like FCM",
  "homepage": "https://github.com/ker0x/cakephp-push",
  "keywords": [
    "cakephp",
    "push",
    "notification",
    "fcm"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Romain Monteil",
      "email": "monteil.romain@gmail.com",
      "homepage": "http://kerox.fr",
      "role": "Author"
    }
  ],
  "support": {
    "issues": "https://github.com/ker0x/cakephp-push/issues",
    "source": "https://github.com/ker0x/cakephp-push"
  },
  "require": {
    "php": ">=7.2.0",
    "cakephp/cakephp": "^4.0"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "fig-r/psr2r-sniffer": "dev-master",
    "phpunit/phpunit": "^8.5 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "Kerox\\Push\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kerox\\Push\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "cs-check": "phpcs -p -s --standard=vendor/fig-r/psr2r-sniffer/PSR2R/ruleset.xml --extensions=php src/ tests/ config/",
    "cs-fix": "phpcbf -p --standard=vendor/fig-r/psr2r-sniffer/PSR2R/ruleset.xml --extensions=php src/ tests/ config/"
  },
  "config": {
    "sort-packages": true
  }
}
